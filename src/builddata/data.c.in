#include "builddata/data.h"

static const char olsrd_version[] = "${OLSRD_VERSION}";
static const char olsrd_git[] = "${OLSRD_SRC_GIT}";
static const char olsrd_change[] = "${OLSRD_SRC_CHANGE}";
static const char build_date[] = __DATE__" "__TIME__;
static const char build_host[] = "${CMAKE_SYSTEM}";
static const char shared_prefix[] = "${CMAKE_SHARED_LIBRARY_PREFIX}";
static const char shared_suffix[] = "${CMAKE_SHARED_LIBRARY_SUFFIX}";

const char *
olsr_builddata_get_version(void) {
  return olsrd_version;
}

const char *
olsr_builddata_get_git_commit(void) {
  return olsrd_git;
}

const char *
olsr_builddata_get_git_change(void) {
  return olsrd_change;
}

const char *
olsr_builddata_get_builddate(void) {
  return build_date;
}

const char *
olsr_builddata_get_buildsystem(void) {
  return build_host;
}

const char *
olsr_builddata_get_sharedlibrary_prefix(void) {
  return shared_prefix;
}

const char *
olsr_builddata_get_sharedlibrary_suffix(void) {
  return shared_suffix;
}
